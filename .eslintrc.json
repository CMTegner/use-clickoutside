{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 5
    },
    "plugins": [
        "prettier"
    ],
    "root": true,
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "tabWidth": 4,
                "singleQuote": true,
                "trailingComma": "es5"
            }
        ]
    }
}